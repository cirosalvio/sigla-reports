		<variableExpression><![CDATA[$F{STANZIAMENTO}]]></variableExpression>
	</variable>
	<variable name="SUM_VARIAZIONI_GAE" class="java.math.BigDecimal" resetType="Group" resetGroup="GAE" calculation="Sum">
		<variableExpression><![CDATA[$F{VARIAZIONI}]]></variableExpression>
	</variable>
