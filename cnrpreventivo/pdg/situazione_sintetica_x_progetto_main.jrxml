		<variableExpression><![CDATA[$F{IMPACC}]]></variableExpression>
	</variable>
	<variable name="SUM_PAGRIS_GAE" class="java.math.BigDecimal" resetType="Group" resetGroup="GAE" calculation="Sum">
		<variableExpression><![CDATA[$F{PAGRIS}]]></variableExpression>
	</variable>
	<variable name="SUM_STANZIAMENTO_ESERCIZIO" class="java.math.BigDecimal" resetType="Group" resetGroup="ESERCIZIO_VOCE" calculation="Sum">
		<variableExpression><![CDATA[$F{STANZIAMENTO}]]></variableExpression>
	</variable>
	<variable name="SUM_VARIAZIONI_ESERCIZIO" class="java.math.BigDecimal" resetType="Group" resetGroup="ESERCIZIO_VOCE" calculation="Sum">
		<variableExpression><![CDATA[$F{VARIAZIONI}]]></variableExpression>
	</variable>
	<variable name="SUM_IMPACC_ESERCIZIO" class="java.math.BigDecimal" resetType="Group" resetGroup="ESERCIZIO_VOCE" calculation="Sum">
		<variableExpression><![CDATA[$F{IMPACC}]]></variableExpression>
	</variable>
	<variable name="SUM_PAGRIS_ESERCIZIO" class="java.math.BigDecimal" resetType="Group" resetGroup="ESERCIZIO_VOCE" calculation="Sum">
		<variableExpression><![CDATA[$F{PAGRIS}]]></variableExpression>
	</variable>
	<variable name="SUM_STANZIAMENTO_LIV2" class="java.math.BigDecimal" resetType="Group" resetGroup="PROGETTO_LIV2" calculation="Sum">
		<variableExpression><![CDATA["E".equals($F{TIPO})?$F{STANZIAMENTO}:$F{STANZIAMENTO}.negate()]]></variableExpression>
	</variable>
	<variable name="SUM_VARIAZIONI_LIV2" class="java.math.BigDecimal" resetType="Group" resetGroup="PROGETTO_LIV2" calculation="Sum">
		<variableExpression><![CDATA["E".equals($F{TIPO})?$F{VARIAZIONI}:$F{VARIAZIONI}.negate()]]></variableExpression>
	</variable>
	<variable name="SUM_IMPACC_LIV2" class="java.math.BigDecimal" resetType="Group" resetGroup="PROGETTO_LIV2" calculation="Sum">
		<variableExpression><![CDATA["E".equals($F{TIPO})?$F{IMPACC}:$F{IMPACC}.negate()]]></variableExpression>
