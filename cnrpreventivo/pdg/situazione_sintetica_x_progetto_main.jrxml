	<queryString>
		<![CDATA[SELECT p.tipo, p.esercizio_res, p.cd_linea_attivita cd_gae,
       CASE WHEN $P{P_ROTTURA_GAE}='S'
	  THEN (SELECT nvl(q.ds_linea_attivita,q.denominazione) from linea_attivita q
	        WHERE q.cd_centro_responsabilita = $P{P_CENTRO_RESPONSABILITA}
	        AND   q.cd_linea_attivita = p.cd_linea_attivita)
            ELSE 'DS_GAE_UNICA'
            END ds_gae,
       p.cd_elemento_voce,
       (SELECT b.ds_elemento_voce FROM ELEMENTO_VOCE B
        WHERE B.TI_GESTIONE = P.TIPO
        AND B.CD_ELEMENTO_VOCE = P.CD_ELEMENTO_VOCE
        and ROWNUM<2) ds_elemento_voce,
       p.stanziamento_acc, p.variazioni_acc,
       p.stanziamento, p.variazioni,
       p.impacc, p.pagris, p.nummov
FROM (SELECT  a.tipo, a.esercizio_res, a.cd_linea_attivita, a.cd_elemento_voce,
              sum(a.stanziamento_acc) stanziamento_acc,
              sum(a.variazioni_acc) variazioni_acc,
              sum(a.stanziamento) stanziamento,
              sum(a.variazioni) variazioni,
              sum(CASE WHEN a.esercizio=$P{P_ESERCIZIO}
                       THEN a.impacc
                       ELSE (CASE WHEN a.impacc<a.pagris
                                  THEN a.impacc
			   ELSE a.pagris
                                  END)
                       END) impacc,
              sum(a.pagris) pagris,
              sum(a.contamov) nummov
      FROM
        (SELECT  esercizio,
	       CASE WHEN $P{P_ROTTURA_ANNO}='S'
	            THEN esercizio_res
	            ELSE 0
                      END esercizio_res,
                 'E' tipo,
  	       CASE WHEN $P{P_ROTTURA_GAE}='S'
                      THEN lda
  	            ELSE 'GAE_UNICA'
                      END cd_linea_attivita,
                 CASE WHEN $P{P_ROTTURA_VOCE}='S'
		  THEN NVL(getCurrentCdElementoVoce($P{P_ESERCIZIO}, esercizio_res, 'E' , cd_elemento_voce),
 			 cd_elemento_voce)
                      ELSE 'VOCE_UNICA'
                      END cd_elemento_voce,
	       0 stanziamento_acc, 0 variazioni_acc,
                 CASE WHEN esercizio = esercizio_res
                      THEN NVL(STANZ_INI,0)
                      ELSE 0
                      END stanziamento,
                 CASE WHEN esercizio = esercizio_res
                      THEN NVL(VAR_PIU,0)-NVL(VAR_MENO,0)
                      ELSE 0
                      END variazioni,
                 CASE WHEN esercizio = esercizio_res
                      THEN NVL(ACC_COMP,0)
                      ELSE NVL(ACC_RES_PRO,0)+
                           NVL(VAR_PIU_RES_PRO,0)-NVL(VAR_MENO_RES_PRO,0)
                      END impacc,
                 CASE WHEN esercizio = esercizio_res
                      THEN NVL(RISCOSSO_COMP,0)
                      ELSE NVL(RISCOSSO_RES,0)
                      END pagris,
                 CASE WHEN esercizio = esercizio_res
                      THEN CASE WHEN NVL(STANZ_INI,0)!=0 OR NVL(VAR_PIU,0)!=0 OR
                           	      NVL(VAR_MENO,0)!=0 OR NVL(ACC_COMP,0)!=0 OR NVL(RISCOSSO_COMP,0)!=0
			 THEN 1
                                ELSE 0
                                END
                      ELSE CASE WHEN NVL(ACC_RES_PRO,0)!=0 OR NVL(VAR_PIU_RES_PRO,0)!=0 OR
                                     NVL(VAR_MENO_RES_PRO,0)!=0 OR NVL(RISCOSSO_RES,0)!=0
			 THEN 1
                                ELSE 0
                                END
                      END contamov
         FROM V_CONS_DISP_COMP_RES_ENT
         WHERE $P{P_TIPO} = 'E'
         AND   cdr = $P{P_CENTRO_RESPONSABILITA}
         AND   lda in (SELECT cd_linea_attivita
                       FROM v_linea_attivita_valida
                       WHERE cd_centro_responsabilita = $P{P_CENTRO_RESPONSABILITA}
                       AND   pg_progetto=$P{P_PG_PROGLIV2}
                       AND   cd_linea_attivita=NVL(decode($P{P_GAE},'*',null,$P{P_GAE}), cd_linea_attivita))
         UNION ALL
         SELECT  esercizio,
	       CASE WHEN $P{P_ROTTURA_ANNO}='S'
	            THEN esercizio_res
	            ELSE 0
                      END esercizio_res,
	      'S' tipo,
  	       CASE WHEN $P{P_ROTTURA_GAE}='S'
                      THEN lda
  	            ELSE 'GAE_UNICA'
                      END cd_linea_attivita,
                 CASE WHEN $P{P_ROTTURA_VOCE}='S'
                      THEN NVL(getCurrentCdElementoVoce($P{P_ESERCIZIO}, esercizio_res, 'S' , cd_elemento_voce),
 			cd_elemento_voce)
                      ELSE 'VOCE_UNICA'
                      END cd_elemento_voce,
	      0 stanziamento_acc, 0 variazioni_acc,
                CASE WHEN esercizio = esercizio_res
                     THEN NVL(STANZ_INI,0)
                     ELSE 0
                     END stanziamento,
                CASE WHEN esercizio = esercizio_res
                     THEN NVL(VAR_PIU,0)-NVL(VAR_MENO,0)
                     ELSE NVL(VAR_PIU_STANZ_RES_IMP,0)-NVL(VAR_MENO_STANZ_RES_IMP,0)
                     END variazioni,
                CASE WHEN esercizio = esercizio_res
                     THEN NVL(OBB_COMP,0)
                     ELSE NVL(OBB_RES_IMP,0)+NVL(OBB_RES_PRO,0)+
                          NVL(VAR_PIU_RES_PRO,0)-NVL(VAR_MENO_RES_PRO,0)
                     END impegnato,
                CASE WHEN esercizio = esercizio_res
                     THEN NVL(PAGATO_COMP,0)
                     ELSE NVL(PAGATO_RES,0)
                     END pagato,
                 CASE WHEN esercizio = esercizio_res
                      THEN CASE WHEN NVL(STANZ_INI,0)!=0 OR
                                     NVL(VAR_PIU,0)!=0 OR NVL(VAR_MENO,0)!=0 OR
                                     NVL(OBB_COMP,0)!=0 OR NVL(PAGATO_COMP,0)!=0
			 THEN 1
                                ELSE 0
                                END
                      ELSE CASE WHEN NVL(VAR_PIU_STANZ_RES_IMP,0)!=0 OR NVL(VAR_MENO_STANZ_RES_IMP,0)!=0 OR
                                     NVL(OBB_RES_IMP,0)!=0 OR NVL(OBB_RES_PRO,0)!= 0 OR
                                     NVL(VAR_PIU_RES_PRO,0)!=0 OR NVL(VAR_MENO_RES_PRO,0)!=0 OR
                                     NVL(PAGATO_RES,0)!=0
			 THEN 1
                                ELSE 0
                                END
                      END contamov
         FROM V_CONS_DISP_COMP_RES
         WHERE $P{P_TIPO} = 'S'
         AND   cdr = $P{P_CENTRO_RESPONSABILITA}
         AND   lda in (SELECT cd_linea_attivita
                       FROM v_linea_attivita_valida
                       WHERE cd_centro_responsabilita = $P{P_CENTRO_RESPONSABILITA}
                       AND   pg_progetto=$P{P_PG_PROGLIV2}
                       AND   cd_linea_attivita=NVL(decode($P{P_GAE},'*',null,$P{P_GAE}), cd_linea_attivita))
         UNION ALL
         SELECT  esercizio,
	       CASE WHEN $P{P_ROTTURA_ANNO}='S'
	            THEN esercizio
	            ELSE 0
                      END esercizio_res,
	      'S' tipo,
  	       CASE WHEN $P{P_ROTTURA_GAE}='S'
                      THEN cd_linea_attivita
  	            ELSE 'GAE_UNICA'
                      END cd_linea_attivita,
                 CASE WHEN $P{P_ROTTURA_VOCE}='S'
                      THEN NVL(getCurrentCdElementoVoce($P{P_ESERCIZIO}, esercizio, 'S' , cd_elemento_voce),
 			cd_elemento_voce)
                      ELSE 'VOCE_UNICA'
                      END cd_elemento_voce,
	       DECODE(cd_cdr_assegnatario_clgs,
                        NULL, NVL (im_spese_gest_accentrata_int, 0)
                            + NVL (im_spese_gest_accentrata_est, 0),
                        0) stanziamento_acc, 0 variazioni_acc,
                 0 stanziamento, 0 variazioni, 0 impegnato, 0 pagato,
                 CASE WHEN NVL(im_spese_gest_accentrata_int,0)!=0 OR
                           NVL(im_spese_gest_accentrata_est,0)!=0
		 THEN 1
                      ELSE 0
                      END contamov
         FROM pdg_modulo_spese_gest
         WHERE $P{P_TIPO} = 'S'
         AND   cd_cdr_assegnatario = $P{P_CENTRO_RESPONSABILITA}
         AND   cd_linea_attivita in (SELECT cd_linea_attivita
                                     FROM v_linea_attivita_valida
                                     WHERE cd_centro_responsabilita = $P{P_CENTRO_RESPONSABILITA}
                                     AND   pg_progetto=$P{P_PG_PROGLIV2}
                                     AND   cd_linea_attivita=NVL(decode($P{P_GAE},'*',null,$P{P_GAE}), cd_linea_attivita))
         UNION ALL
         SELECT  pdgvar.esercizio,
	       CASE WHEN $P{P_ROTTURA_ANNO}='S'
	            THEN pdgvar.esercizio
	            ELSE 0
                      END esercizio_res,
	      'S' tipo,
  	       CASE WHEN $P{P_ROTTURA_GAE}='S'
                      THEN pdgvarriga.cd_linea_attivita
  	            ELSE 'GAE_UNICA'
                      END cd_linea_attivita,
                 CASE WHEN $P{P_ROTTURA_VOCE}='S'
                      THEN NVL(getCurrentCdElementoVoce($P{P_ESERCIZIO}, pdgvar.esercizio, 'S' , pdgvarriga.cd_elemento_voce),
 			pdgvarriga.cd_elemento_voce)
                      ELSE 'VOCE_UNICA'
                      END cd_elemento_voce,
	       0 stanziamento_acc,
                 DECODE(pdgvarriga.cd_cdr_assegnatario_clgs,
                        NULL, NVL (pdgvarriga.im_spese_gest_accentrata_int, 0)
                            + NVL (pdgvarriga.im_spese_gest_accentrata_est, 0),
                        0) variazioni_acc,
                 0 stanziamento, 0 variazioni, 0 impegnato, 0 pagato,
                 CASE WHEN NVL(im_spese_gest_accentrata_int,0)!=0 OR
                           NVL(im_spese_gest_accentrata_est,0)!=0
		 THEN 1
                      ELSE 0
                      END contamov
         FROM pdg_variazione pdgvar,
              pdg_variazione_riga_gest pdgvarriga
         WHERE $P{P_TIPO} = 'S'
         AND   pdgvar.esercizio = pdgvarriga.esercizio
         AND   pdgvar.pg_variazione_pdg = pdgvarriga.pg_variazione_pdg
         AND   pdgvar.stato IN ('APP', 'APF')
         AND   pdgvarriga.cd_cdr_assegnatario = $P{P_CENTRO_RESPONSABILITA}
         AND   pdgvarriga.cd_linea_attivita in (SELECT cd_linea_attivita
                                     FROM v_linea_attivita_valida
                                     WHERE cd_centro_responsabilita = $P{P_CENTRO_RESPONSABILITA}
                                     AND   pg_progetto=$P{P_PG_PROGLIV2}
                                     AND   cd_linea_attivita=NVL(decode($P{P_GAE},'*',null,$P{P_GAE}), cd_linea_attivita))
         UNION ALL
         SELECT  c.esercizio,
	       CASE WHEN $P{P_ROTTURA_ANNO}='S'
	            THEN c.esercizio_originale
	            ELSE 0
                      END esercizio_res,
	      'S' tipo,
  	       CASE WHEN $P{P_ROTTURA_GAE}='S'
                      THEN a.CD_LINEA_ATTIVITA
  	            ELSE 'GAE_UNICA'
                      END cd_linea_attivita,
                 CASE WHEN $P{P_ROTTURA_VOCE}='S'
                      THEN NVL(getCurrentCdElementoVoce($P{P_ESERCIZIO}, c.esercizio_originale, 'S', c.cd_elemento_voce),
 		      c.cd_elemento_voce)
                      ELSE 'VOCE_UNICA'
                      END cd_elemento_voce,
 	       0 stanziamento_acc, 0 variazioni_acc,
                 0 stanziamento, -(im_voce-(im_voce*(IM_ASSOCIATO_DOC_CONTABILE/IM_SCADENZA))) variazioni,
                 0 impegnato, 0 pagato, 1 contamov
         FROM obbligazione_scad_voce a, obbligazione_scadenzario b, obbligazione c
         WHERE $P{P_TIPO} = 'S'
         AND   a.cd_cds = b.cd_cds
         and   a.esercizio=b.esercizio
         and   a.esercizio_originale=b.esercizio_originale
         and   a.pg_obbligazione = b.pg_obbligazione
         and   a.pg_obbligazione_scadenzario = b.pg_obbligazione_scadenzario
         and   a.cd_cds = c.cd_cds
         and   a.esercizio=c.esercizio
         and   a.esercizio_originale=c.esercizio_originale
         and   a.pg_obbligazione = c.pg_obbligazione
         and   c.cd_elemento_voce_next is not null
         and   nvl(b.im_scadenza,0)!=0
         and   a.cd_centro_responsabilita = $P{P_CENTRO_RESPONSABILITA}
         and   a.cd_linea_attivita in (SELECT cd_linea_attivita
                                       FROM v_linea_attivita_valida
                                       WHERE cd_centro_responsabilita = $P{P_CENTRO_RESPONSABILITA}
                                       AND   pg_progetto=$P{P_PG_PROGLIV2}
                                       AND   cd_linea_attivita=NVL(decode($P{P_GAE},'*',null,$P{P_GAE}), cd_linea_attivita))
         UNION ALL
         SELECT  c.esercizio,
	       CASE WHEN $P{P_ROTTURA_ANNO}='S'
	            THEN c.esercizio_originale
	            ELSE 0
                      END esercizio_res,
	      'S' tipo,
  	       CASE WHEN $P{P_ROTTURA_GAE}='S'
                      THEN a.CD_LINEA_ATTIVITA
  	            ELSE 'GAE_UNICA'
                      END cd_linea_attivita,
                 CASE WHEN $P{P_ROTTURA_VOCE}='S'
                      THEN cd_elemento_voce_next
                      ELSE 'VOCE_UNICA'
                      END cd_elemento_voce,
	       0 stanziamento_acc, 0 variazioni_acc,
	       0 stanziamento, im_voce-(im_voce*(IM_ASSOCIATO_DOC_CONTABILE/IM_SCADENZA)) variazioni,
                 0 impegnato, 0 pagato, 1 contamov
         FROM obbligazione_scad_voce a, obbligazione_scadenzario b, obbligazione c
         WHERE $P{P_TIPO} = 'S'
         AND   a.cd_cds = b.cd_cds
         and   a.esercizio=b.esercizio
         and   a.esercizio_originale=b.esercizio_originale
         and   a.pg_obbligazione = b.pg_obbligazione
         and   a.pg_obbligazione_scadenzario = b.pg_obbligazione_scadenzario
         and   a.cd_cds = c.cd_cds
         and   a.esercizio=c.esercizio
         and   a.esercizio_originale=c.esercizio_originale
         and   a.pg_obbligazione = c.pg_obbligazione
         and   c.cd_elemento_voce_next is not null
         and   nvl(b.im_scadenza,0)!=0
         and   a.cd_centro_responsabilita = $P{P_CENTRO_RESPONSABILITA}
         and   a.cd_linea_attivita in (SELECT cd_linea_attivita
                                       FROM v_linea_attivita_valida
                                       WHERE cd_centro_responsabilita = $P{P_CENTRO_RESPONSABILITA}
                                       AND   pg_progetto=$P{P_PG_PROGLIV2}
                                       AND   cd_linea_attivita=NVL(decode($P{P_GAE},'*',null,$P{P_GAE}), cd_linea_attivita))
         UNION ALL
         SELECT  c.esercizio,
	       CASE WHEN $P{P_ROTTURA_ANNO}='S'
	            THEN c.esercizio_originale
	            ELSE 0
                      END esercizio_res,
	      'E' tipo,
  	       CASE WHEN $P{P_ROTTURA_GAE}='S'
                      THEN a.CD_LINEA_ATTIVITA
  	            ELSE 'GAE_UNICA'
                      END cd_linea_attivita,
                 CASE WHEN $P{P_ROTTURA_VOCE}='S'
                      THEN NVL(getCurrentCdElementoVoce($P{P_ESERCIZIO}, c.esercizio_originale, 'E', c.cd_elemento_voce),
 		      c.cd_elemento_voce)
                      ELSE 'VOCE_UNICA'
                      END cd_elemento_voce,
	       0 stanziamento_acc, 0 variazioni_acc,
	       0 stanziamento, -(im_voce-(im_voce*(IM_ASSOCIATO_DOC_CONTABILE/IM_SCADENZA))) variazioni,
                 0 impegnato, 0 pagato, 1 contamov
         FROM accertamento_scad_voce a, accertamento_scadenzario b, accertamento c
         WHERE $P{P_TIPO} = 'E'
         AND   a.cd_cds = b.cd_cds
         and   a.esercizio=b.esercizio
         and   a.esercizio_originale=b.esercizio_originale
         and   a.pg_accertamento = b.pg_accertamento
         and   a.pg_accertamento_scadenzario = b.pg_accertamento_scadenzario
         and   a.cd_cds = c.cd_cds
         and   a.esercizio=c.esercizio
         and   a.esercizio_originale=c.esercizio_originale
         and   a.pg_accertamento = c.pg_accertamento
         and   c.cd_elemento_voce_next is not null
         and   nvl(b.im_scadenza,0)!=0
         and   a.cd_centro_responsabilita = $P{P_CENTRO_RESPONSABILITA}
         and   a.cd_linea_attivita in (SELECT cd_linea_attivita
                                       FROM v_linea_attivita_valida
                                       WHERE cd_centro_responsabilita = $P{P_CENTRO_RESPONSABILITA}
                                       AND   pg_progetto=$P{P_PG_PROGLIV2}
                                       AND   cd_linea_attivita=NVL(decode($P{P_GAE},'*',null,$P{P_GAE}), cd_linea_attivita))
         UNION ALL
         SELECT  c.esercizio,
	       CASE WHEN $P{P_ROTTURA_ANNO}='S'
	            THEN c.esercizio_originale
	            ELSE 0
                      END esercizio_res,
	      'E' tipo,
  	       CASE WHEN $P{P_ROTTURA_GAE}='S'
                      THEN a.CD_LINEA_ATTIVITA
  	            ELSE 'GAE_UNICA'
                      END cd_linea_attivita,
                 CASE WHEN $P{P_ROTTURA_VOCE}='S'
                      THEN cd_elemento_voce_next
                      ELSE 'VOCE_UNICA'
                      END cd_elemento_voce,
	      0 stanziamento_acc, 0 variazioni_acc,
                 0 stanziamento, im_voce-(im_voce*(IM_ASSOCIATO_DOC_CONTABILE/IM_SCADENZA)) variazioni,
                 0 impegnato, 0 pagato, 1 contamov
         FROM accertamento_scad_voce a, accertamento_scadenzario b, accertamento c
         WHERE $P{P_TIPO} = 'E'
         AND   a.cd_cds = b.cd_cds
         and   a.esercizio=b.esercizio
         and   a.esercizio_originale=b.esercizio_originale
         and   a.pg_accertamento = b.pg_accertamento
         and   a.pg_accertamento_scadenzario = b.pg_accertamento_scadenzario
         and   a.cd_cds = c.cd_cds
         and   a.esercizio=c.esercizio
         and   a.esercizio_originale=c.esercizio_originale
         and   a.pg_accertamento = c.pg_accertamento
         and   c.cd_elemento_voce_next is not null
         and   nvl(b.im_scadenza,0)!=0
         and   a.cd_centro_responsabilita = $P{P_CENTRO_RESPONSABILITA}
         and   a.cd_linea_attivita in (SELECT cd_linea_attivita
                                       FROM v_linea_attivita_valida
                                       WHERE cd_centro_responsabilita = $P{P_CENTRO_RESPONSABILITA}
                                       AND   pg_progetto=$P{P_PG_PROGLIV2}
                                       AND   cd_linea_attivita=NVL(decode($P{P_GAE},'*',null,$P{P_GAE}), cd_linea_attivita))
         UNION ALL
         SELECT a.esercizio,
	       CASE WHEN $P{P_ROTTURA_ANNO}='S'
	            THEN 2005
	            ELSE 0
                      END esercizio_res,
	      'S' tipo,
  	       CASE WHEN $P{P_ROTTURA_GAE}='S'
                      THEN a.CD_LINEA_ATTIVITA
  	            ELSE 'GAE_UNICA'
                      END cd_linea_attivita,
                 CASE WHEN $P{P_ROTTURA_VOCE}='S'
                      THEN NVL(getCurrentCdElementoVoce($P{P_ESERCIZIO}, 2005, 'S' , a.cd_elemento_voce),
 	          a.cd_elemento_voce)
                      ELSE 'VOCE_UNICA'
                      END cd_elemento_voce,
	      0 stanziamento_acc, 0 variazioni_acc,
                NVL(a.im_residuo, 0), 0 variazioni, 0 impegnato, 0 pagato, 1 contamov
         FROM pdg_residuo_det a
         WHERE $P{P_TIPO} = 'S'
         AND   a.esercizio = 2005
         AND   a.stato != 'A'
         AND   a.cd_cdr_linea = $P{P_CENTRO_RESPONSABILITA}
         AND   a.cd_linea_attivita in (SELECT cd_linea_attivita
                                       FROM v_linea_attivita_valida
                                       WHERE cd_centro_responsabilita = $P{P_CENTRO_RESPONSABILITA}
                                       AND   pg_progetto=$P{P_PG_PROGLIV2}
                                       AND   cd_linea_attivita=NVL(decode($P{P_GAE},'*',null,$P{P_GAE}), cd_linea_attivita))) a
      GROUP BY a.tipo, a.esercizio_res, a.cd_linea_attivita, a.cd_elemento_voce) P
WHERE (p.stanziamento != 0 OR p.variazioni != 0 OR p.impacc != 0 OR p.pagris != 0 or p.nummov!=0)
AND ($P{P_SOLO_GAE_ATTIVE}='N' OR
     EXISTS(SELECT '1' FROM VOCE_F_SALDI_CDR_LINEA SALDI
            WHERE SALDI.ESERCIZIO = $P{P_ESERCIZIO}
            AND   SALDI.ESERCIZIO_RES = P.ESERCIZIO_RES
            AND   SALDI.CD_CENTRO_RESPONSABILITA = $P{P_CENTRO_RESPONSABILITA}
            AND   SALDI.CD_LINEA_ATTIVITA = P.CD_LINEA_ATTIVITA
            AND   SALDI.TI_GESTIONE = P.TIPO
            AND   SALDI.CD_ELEMENTO_VOCE = P.CD_ELEMENTO_VOCE))
ORDER BY p.tipo, p.cd_linea_attivita,
CASE WHEN $P{P_ORDINE}='V' THEN p.cd_elemento_voce ELSE TO_CHAR(p.esercizio_res) END,
CASE WHEN $P{P_ORDINE}='V' THEN TO_CHAR(p.esercizio_res) ELSE p.cd_elemento_voce END]]>
