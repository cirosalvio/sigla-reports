	<queryString>
		<![CDATA[SELECT distinct A.CD_CENTRO_RESPONSABILITA, A.PG_PROGETTO PG_PROGLIV2,
B.CD_PROGETTO CD_PROGLIV2, B.DS_PROGETTO DS_PROGLIV2,
B.P_CD_PROGETTO CD_PROGLIV1, B.P_DS_PROGETTO DS_PROGLIV1
FROM V_LINEA_ATTIVITA_VALIDA A, V_PROGETTO_PADRE B
WHERE A.ESERCIZIO = $P{P_ESERCIZIO}
AND ($P{P_UO} IS NULL OR A.CD_CENTRO_RESPONSABILITA LIKE $P{P_UO}||'%')
AND ($P{P_PROGETTO} IS NULL OR A.PG_PROGETTO=$P{P_PROGETTO})
AND A.ESERCIZIO >= 2016
AND A.ESERCIZIO = B.ESERCIZIO
AND A.PG_PROGETTO = B.PG_PROGETTO
AND B.TIPO_FASE='X']]>
	</queryString>
