	</variable>
	<variable name="TOTALE_RITENUTA_PERCIPIENTE" class="java.math.BigDecimal">
		<variableExpression><![CDATA[$V{TOTALE_LORDO_PERCIPIENTE}.subtract($V{TOTALE_NETTO_PERCIPIENTE})]]></variableExpression>
	</variable>
