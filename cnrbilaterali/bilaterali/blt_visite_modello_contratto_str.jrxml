	<detail>
		<band height="49" splitType="Prevent">
			<textField isStretchWithOverflow="true" isBlankWhenNull="true">
				<reportElement key="textField-2" style="TNR11" x="-1" y="35" width="465" height="14"/>
				<textElement textAlignment="Center">
					<font fontName="Times New Roman" size="10"/>
				</textElement>
				<textFieldExpression class="java.lang.String"><![CDATA["Within the framework of the Agreement on Scientific Cooperation signed between the CNR and the "+$F{NOME_ENTE_STR}+" ("+$F{ACRONIMO_ENTE_STR}+") - "+$F{DS_NAZIONE_ENG}+""]]></textFieldExpression>
			</textField>
			<textField isStretchWithOverflow="true" isBlankWhenNull="true">
				<reportElement key="textField-2" style="TNR11" x="1" y="0" width="465" height="14"/>
				<textElement textAlignment="Center">
					<font fontName="Times New Roman" size="10"/>
				</textElement>
				<textFieldExpression class="java.lang.String"><![CDATA["TEMPORARY INDEPENDENT WORK SERVICE AGREEMENT FOR PROFESSIONAL SERVICES RENDERED N ACCORDANCE WITH CURRENT REGULATIONS OF\nArticles 2222 – 2228 of the Italian Civil Code"]]></textFieldExpression>
			</textField>
		</band>
		<band height="20" splitType="Prevent">
			<textField isStretchWithOverflow="true" isBlankWhenNull="true">
				<reportElement key="textField-2" style="TNR11" x="0" y="0" width="465" height="14"/>
				<textElement textAlignment="Center">
					<font fontName="Times New Roman" size="10" isBold="false"/>
				</textElement>
				<textFieldExpression class="java.lang.String"><![CDATA["BETWEEN"]]></textFieldExpression>
			</textField>
		</band>
		<band height="20" splitType="Prevent">
			<textField isStretchWithOverflow="true" isBlankWhenNull="true">
				<reportElement key="textField-2" style="TNR11" x="0" y="0" width="465" height="14"/>
				<textElement textAlignment="Justified">
					<font fontName="Times New Roman" size="10"/>
				</textElement>
				<textFieldExpression class="java.lang.String"><![CDATA["The National Research Council of Italy, Fiscal Code No 80054330586, which is based in Rome, at 7, Piazzale Aldo Moro, here represented by the Director of the International Agreements and Relations Office, in the person of Dr. Virginia Coda Nunziante, born in Rome on 18/04/1966, hence referred to as “CNR”"]]></textFieldExpression>
			</textField>
		</band>
		<band height="20" splitType="Prevent">
			<textField isStretchWithOverflow="true" isBlankWhenNull="true">
				<reportElement key="textField-2" style="TNR11" x="0" y="0" width="465" height="14"/>
				<textElement textAlignment="Center">
					<font fontName="Times New Roman" size="10" isBold="false"/>
				</textElement>
				<textFieldExpression class="java.lang.String"><![CDATA["AND"]]></textFieldExpression>
			</textField>
		</band>
		<band height="20" splitType="Prevent">
			<textField isStretchWithOverflow="true" isBlankWhenNull="true">
				<reportElement key="textField-2" style="TNR11" x="0" y="0" width="465" height="14"/>
				<textElement textAlignment="Justified">
					<font fontName="Times New Roman" size="10"/>
				</textElement>
				<textFieldExpression class="java.lang.String"><![CDATA[""+($F{SESSO_AUTORIZZATO}.equals("M")?"Mr":"Ms")+" "+$F{DS_TERZO_AUTORIZZATO}.toUpperCase()+", born in "+$F{COMUNE_NASCITA}+" ("+$F{DS_NAZIONE}+") on "+new SimpleDateFormat("dd/MM/yyyy").format($F{DT_NASCITA})+", who permanently resides in "+$F{COMUNE_FISCALE}+", "+$F{VIA_FISCALE}+", citizenship "+$F{DS_AGGETTIVO_NAZIONAL}+" hence referred to as the “Contractor”"]]></textFieldExpression>
			</textField>
		</band>
		<band height="20" splitType="Prevent">
			<textField isStretchWithOverflow="true" isBlankWhenNull="true">
				<reportElement key="textField-2" style="TNR11" x="0" y="0" width="465" height="14"/>
				<textElement textAlignment="Center">
					<font fontName="Times New Roman" size="10" isBold="false"/>
				</textElement>
				<textFieldExpression class="java.lang.String"><![CDATA["PRELIMINARY REMARKS"]]></textFieldExpression>
			</textField>
		</band>
		<band height="20" splitType="Prevent">
			<textField isStretchWithOverflow="true" isBlankWhenNull="true">
				<reportElement key="textField-2" style="TNR11" x="0" y="0" width="13" height="14"/>
				<textElement textAlignment="Center">
					<font fontName="Times New Roman" size="10" isBold="false"/>
				</textElement>
				<textFieldExpression class="java.lang.String"><![CDATA["-"]]></textFieldExpression>
			</textField>
			<textField isStretchWithOverflow="true" isBlankWhenNull="true">
				<reportElement key="textField-2" style="TNR11" x="13" y="0" width="452" height="14"/>
				<textElement textAlignment="Justified">
					<font fontName="Times New Roman" size="10"/>
				</textElement>
				<textFieldExpression class="java.lang.String"><![CDATA["an Agreement of Scientific Cooperation is currently in force between CNR and "+$F{ACRONIMO_ENTE_STR}+", signed on "+new SimpleDateFormat("dd/MM/yyyy").format($F{DATA_FIRMA_ACCORDO})+", which includes the exchange of researchers for carrying out joint bilateral projects between the two Academies and the corresponding Addendum signed on "+new SimpleDateFormat("dd/MM/yyyy").format($F{DATA_FIRMA_ADDENDUM})+";"]]></textFieldExpression>
			</textField>
		</band>
		<band height="20" splitType="Prevent">
			<textField isStretchWithOverflow="true" isBlankWhenNull="true">
				<reportElement key="textField-2" style="TNR11" x="0" y="0" width="13" height="14"/>
				<textElement textAlignment="Center">
					<font fontName="Times New Roman" size="10" isBold="false"/>
				</textElement>
				<textFieldExpression class="java.lang.String"><![CDATA["-"]]></textFieldExpression>
			</textField>
			<textField isStretchWithOverflow="true" isBlankWhenNull="true">
				<reportElement key="textField-2" style="TNR11" x="13" y="0" width="452" height="14"/>
				<textElement textAlignment="Justified">
					<font fontName="Times New Roman" size="10"/>
				</textElement>
				<textFieldExpression class="java.lang.String"><![CDATA["the above – mentioned activity is not subject to the procedure of internal professionalism verification since the contract itself does not fall under Art. 7, paragraph 6 of the Legislative Decree No 165, of 30/03/2001, but rather it is regulated by the Scientific Cooperation Agreement between CNR and "+$F{ACRONIMO_ENTE_STR}+";"]]></textFieldExpression>
			</textField>
		</band>
		<band height="20" splitType="Prevent">
			<textField isStretchWithOverflow="true" isBlankWhenNull="true">
				<reportElement key="textField-2" style="TNR11" x="0" y="0" width="13" height="14"/>
				<textElement textAlignment="Center">
					<font fontName="Times New Roman" size="10" isBold="false"/>
				</textElement>
				<textFieldExpression class="java.lang.String"><![CDATA["-"]]></textFieldExpression>
			</textField>
			<textField isStretchWithOverflow="true" isBlankWhenNull="true">
				<reportElement key="textField-2" style="TNR11" x="13" y="0" width="452" height="14"/>
				<textElement textAlignment="Justified">
					<font fontName="Times New Roman" size="10"/>
				</textElement>
				<textFieldExpression class="java.lang.String"><![CDATA["the CNR is hereby signing an independent work service agreement for professional services exclusively of a temporary and extraordinary nature;"]]></textFieldExpression>
			</textField>
		</band>
		<band height="20" splitType="Prevent">
			<textField isStretchWithOverflow="true" isBlankWhenNull="true">
				<reportElement key="textField-2" style="TNR11" x="13" y="0" width="452" height="14"/>
				<textElement textAlignment="Justified">
					<font fontName="Times New Roman" size="10"/>
				</textElement>
				<textFieldExpression class="java.lang.String"><![CDATA["the Bilateral Research Project '"+$F{DS_PROGETTO_ENG}+"' (Italian Project Coordinator "+($F{SESSO_AUTORIZZATO}.equals("M")?"Mr":"Ms")+" "+$F{DS_RESPONSABILE_ITA}.toUpperCase()+", "+$F{DS_AGGETTIVO_NAZIONAL}+" Project Coordinator "+($F{SESSO_AUTORIZZATO}.equals("M")?"Mr":"Ms")+" "+$F{DS_RESPONSABILE_STR}.toUpperCase()+") which has been jointly approved by CNR and "+$F{ACRONIMO_ENTE_STR}+" for the "+($F{ANNO_FIN}.subtract($F{ANNO_INI}).compareTo(BigDecimal.ONE)==0?"biennium":($F{ANNO_FIN}.subtract($F{ANNO_INI}).compareTo(new BigDecimal(2))==0?"triennium":"years"))+ " " +$F{ANNO_INI}+"-"+$F{ANNO_FIN}+", involves "+($F{SESSO_AUTORIZZATO}.equals("M")?"Mr":"Ms")+" "+$F{DS_TERZO_AUTORIZZATO}.toUpperCase()+" in the list of candidates selected and approved by "+$F{ACRONIMO_ENTE_STR}+";"]]></textFieldExpression>
			</textField>
			<textField isStretchWithOverflow="true" isBlankWhenNull="true">
				<reportElement key="textField-2" style="TNR11" x="0" y="0" width="13" height="14"/>
				<textElement textAlignment="Center">
					<font fontName="Times New Roman" size="10" isBold="false"/>
				</textElement>
				<textFieldExpression class="java.lang.String"><![CDATA["-"]]></textFieldExpression>
			</textField>
		</band>
		<band height="20" splitType="Prevent">
			<textField isStretchWithOverflow="true" isBlankWhenNull="true">
				<reportElement key="textField-2" style="TNR11" x="13" y="0" width="452" height="14"/>
				<textElement textAlignment="Justified">
					<font fontName="Times New Roman" size="10" isBold="false"/>
				</textElement>
				<textFieldExpression class="java.lang.String"><![CDATA["the CNR with the Official Note, Ref. No. AMMCNT - CNR n. "+$F{NUM_PROT_ATTRIB_INCARICO}+" dated "+new SimpleDateFormat("dd/MM/yyyy").format($F{DT_PROT_ATTRIB_INCARICO})+", has ratified the research stay of "+($F{SESSO_AUTORIZZATO}.equals("M")?"Mr":"Ms")+" "+$F{DS_TERZO_AUTORIZZATO}.toUpperCase()+";"]]></textFieldExpression>
			</textField>
			<textField isStretchWithOverflow="true" isBlankWhenNull="true">
				<reportElement key="textField-2" style="TNR11" x="-1" y="0" width="13" height="14"/>
				<textElement textAlignment="Center">
					<font fontName="Times New Roman" size="10" isBold="false"/>
				</textElement>
				<textFieldExpression class="java.lang.String"><![CDATA["-"]]></textFieldExpression>
			</textField>
		</band>
		<band height="20" splitType="Prevent">
			<textField isStretchWithOverflow="true" isBlankWhenNull="true">
				<reportElement key="textField-2" style="TNR11" x="0" y="6" width="465" height="14"/>
				<textElement textAlignment="Center">
					<font fontName="Times New Roman" size="10" isBold="false"/>
				</textElement>
				<textFieldExpression class="java.lang.String"><![CDATA["IT IS HEREBY AGREED AS FOLLOWS:"]]></textFieldExpression>
			</textField>
		</band>
		<band height="20" splitType="Prevent">
			<textField isStretchWithOverflow="true" isBlankWhenNull="true">
				<reportElement key="textField-2" style="TNR11" x="0" y="6" width="465" height="14"/>
				<textElement textAlignment="Justified">
					<font fontName="Times New Roman" size="10" isBold="false"/>
				</textElement>
				<textFieldExpression class="java.lang.String"><![CDATA["The above – mentioned preliminary remarks constitute an integral part of the present Agreement. "]]></textFieldExpression>
			</textField>
		</band>
		<band height="18" splitType="Prevent">
			<textField isStretchWithOverflow="true" isBlankWhenNull="true">
				<reportElement key="textField-2" style="TNR11" x="0" y="4" width="465" height="14"/>
				<textElement>
					<font fontName="Times New Roman" size="10" isBold="false"/>
				</textElement>
				<textFieldExpression class="java.lang.String"><![CDATA["ART. 1 –  Type of Agreement"]]></textFieldExpression>
			</textField>
		</band>
		<band height="18" splitType="Prevent">
			<textField isStretchWithOverflow="true" isBlankWhenNull="true">
				<reportElement key="textField-2" style="TNR11" x="0" y="0" width="465" height="14"/>
				<textElement textAlignment="Justified">
					<font fontName="Times New Roman" size="10" isBold="false"/>
				</textElement>
				<textFieldExpression class="java.lang.String"><![CDATA["The contracting parties mutually agree to stipulate a temporary independent work service agreement for professional services rendered in accordance with current regulations of Articles 2222 and following of the Italian Civil Code, in compliance with the following terms and conditions."]]></textFieldExpression>
			</textField>
		</band>
		<band height="18" splitType="Prevent">
			<textField isStretchWithOverflow="true" isBlankWhenNull="true">
				<reportElement key="textField-2" style="TNR11" x="0" y="4" width="465" height="14"/>
				<textElement>
					<font fontName="Times New Roman" size="10" isBold="false"/>
				</textElement>
				<textFieldExpression class="java.lang.String"><![CDATA["ART. 2 –  Scope of the Contract "]]></textFieldExpression>
			</textField>
		</band>
		<band height="20" splitType="Prevent">
			<textField isStretchWithOverflow="true" isBlankWhenNull="true">
				<reportElement key="textField-2" style="TNR11" x="0" y="0" width="465" height="14"/>
				<textElement textAlignment="Justified">
					<font fontName="Times New Roman" size="10" isBold="false"/>
				</textElement>
				<textFieldExpression class="java.lang.String"><![CDATA["The “Contractor” undertakes to carry out a research stay of "+$V{numGiorni}+" days, at "+$F{LUOGO_VISITA}+", starting from "+new SimpleDateFormat("dd/MM/yyyy").format($F{DT_INI_VISITA})+" and ending on "+new SimpleDateFormat("dd/MM/yyyy").format($F{DT_FIN_VISITA})+" in order to conduct research activities for the Research Project '"+$F{DS_PROGETTO_ENG}+"', within the framework of the Scientific Cooperation Agreement currently in force between CNR and "+$F{ACRONIMO_ENTE_STR}+" and relative Addendum."]]></textFieldExpression>
			</textField>
		</band>
		<band height="18" splitType="Prevent">
			<textField isStretchWithOverflow="true" isBlankWhenNull="true">
				<reportElement key="textField-2" style="TNR11" x="0" y="4" width="465" height="14"/>
				<textElement>
					<font fontName="Times New Roman" size="10" isBold="false"/>
				</textElement>
				<textFieldExpression class="java.lang.String"><![CDATA["ART. 3 – Terms and Conditions"]]></textFieldExpression>
			</textField>
		</band>
		<band height="18" splitType="Prevent">
			<textField isStretchWithOverflow="true" isBlankWhenNull="true">
				<reportElement key="textField-2" style="TNR11" x="0" y="0" width="465" height="14"/>
				<textElement textAlignment="Justified">
					<font fontName="Times New Roman" size="10" isBold="false"/>
				</textElement>
				<textFieldExpression class="java.lang.String"><![CDATA["The Contractor undertakes to accomplish the task in person, without any subordination or time-keeping obligation, in full technical and logistical independence. The present contract does not entail any kind of hierarchical subordination since the Contractor will not follow precise orders but, on the contrary, will act in full autonomy both in the organization and the management of  "+($F{SESSO_AUTORIZZATO}.equals("M")?"him":"her")+" activity as seen fit, with the sole purpose of achieving the expected results within the above - mentioned Research Project."]]></textFieldExpression>
			</textField>
		</band>
		<band height="18" splitType="Prevent">
			<textField isStretchWithOverflow="true" isBlankWhenNull="true">
				<reportElement key="textField-2" style="TNR11" x="0" y="4" width="465" height="14"/>
				<textElement>
					<font fontName="Times New Roman" size="10" isBold="false"/>
				</textElement>
				<textFieldExpression class="java.lang.String"><![CDATA["ART. 4 – Duration and Starting Date of the Contract "]]></textFieldExpression>
			</textField>
		</band>
		<band height="18" splitType="Prevent">
			<textField isStretchWithOverflow="true" isBlankWhenNull="true">
				<reportElement key="textField-2" style="TNR11" x="0" y="0" width="465" height="14"/>
				<textElement textAlignment="Justified">
					<font fontName="Times New Roman" size="10" isBold="false"/>
				</textElement>
				<textFieldExpression class="java.lang.String"><![CDATA["The present Contract will have a duration of "+$V{numGiorni}+" days, starting from "+new SimpleDateFormat("dd/MM/yyyy").format($F{DT_INI_VISITA})+" and will terminate on "+new SimpleDateFormat("dd/MM/yyyy").format($F{DT_FIN_VISITA})+". The starting date can be postponed only owing to an act of God, and will have to be officially authorized by CNR."]]></textFieldExpression>
			</textField>
		</band>
		<band height="18" splitType="Prevent">
			<textField isStretchWithOverflow="true" isBlankWhenNull="true">
				<reportElement key="textField-2" style="TNR11" x="0" y="4" width="465" height="14"/>
				<textElement>
					<font fontName="Times New Roman" size="10" isBold="false"/>
				</textElement>
				<textFieldExpression class="java.lang.String"><![CDATA["ART. 5 – Remuneration, tax regime and Payment procedures. "]]></textFieldExpression>
			</textField>
		</band>
		<band height="18" splitType="Prevent">
			<textField isStretchWithOverflow="true" isBlankWhenNull="true">
				<reportElement key="textField-2" style="TNR11" x="0" y="0" width="465" height="14"/>
				<textElement textAlignment="Justified">
					<font fontName="Times New Roman" size="10" isBold="false"/>
				</textElement>
				<textFieldExpression class="java.lang.String"><![CDATA["The parties agree that, under the provisions by the Agreement of Scientific Cooperation between CNR and "+$F{ACRONIMO_ENTE_STR}+" and relative Addendum currently in force, the remuneration due to the Contractor for carrying out the above–mentioned temporary and independent undertaking will be a total sum of Euro "+new java.text.DecimalFormat("#,##0.00").format($F{IM_RIMB_PREVISTO})+"."]]></textFieldExpression>
			</textField>
		</band>
		<band height="18" splitType="Prevent">
			<printWhenExpression><![CDATA[$F{FL_ACCETTAZIONE_CONVENZIONE}.equals("N")]]></printWhenExpression>
			<textField isStretchWithOverflow="true" isBlankWhenNull="true">
				<reportElement key="textField-2" style="TNR11" x="0" y="0" width="465" height="14"/>
				<textElement textAlignment="Justified">
					<font fontName="Times New Roman" size="10" isBold="false"/>
				</textElement>
				<textFieldExpression class="java.lang.String"><![CDATA["The Contractor doesn’t wish to avail "+($F{SESSO_AUTORIZZATO}.equals("M")?"himself":"herself")+" of the appropriate Convention for the Avoidance of Double Taxation."]]></textFieldExpression>
			</textField>
		</band>
		<band height="18" splitType="Prevent">
			<printWhenExpression><![CDATA[$F{FL_ACCETTAZIONE_CONVENZIONE}.equals("Y")]]></printWhenExpression>
			<textField isStretchWithOverflow="true" isBlankWhenNull="true">
				<reportElement key="textField-2" style="TNR11" x="0" y="0" width="465" height="14"/>
				<textElement textAlignment="Justified">
					<font fontName="Times New Roman" size="10" isBold="false"/>
				</textElement>
				<textFieldExpression class="java.lang.String"><![CDATA["The Contractor wishes to avail "+($F{SESSO_AUTORIZZATO}.equals("M")?"himself":"herself")+" of the appropriate Convention for the Avoidance of Double Taxation."]]></textFieldExpression>
			</textField>
		</band>
		<band height="18" splitType="Prevent">
			<printWhenExpression><![CDATA[$F{FL_PAGAMENTO_CON_BONIFICO}.equals( "Y" ) && $F{FL_PAGAMENTO_FINE_VISITA}.equals( "Y")]]></printWhenExpression>
			<textField isStretchWithOverflow="true" isBlankWhenNull="true">
				<reportElement key="textField-2" style="TNR11" x="0" y="0" width="465" height="14"/>
				<textElement textAlignment="Justified">
					<font fontName="Times New Roman" size="10" isBold="false"/>
				</textElement>
				<textFieldExpression class="java.lang.String"><![CDATA["The sum payable by CNR will be paid at the end of the stay by bank transfer directly onto the Contractor’s own bank account SWIFT Code "+$F{CODICE_SWIFT}+", IBAN Code "+$F{CODICE_IBAN}+"."]]></textFieldExpression>
			</textField>
		</band>
		<band height="18" splitType="Prevent">
			<printWhenExpression><![CDATA[$F{IM_RIMB_SPESE_ANT}== null]]></printWhenExpression>
			<textField isStretchWithOverflow="true" isBlankWhenNull="true">
				<reportElement key="textField-2" style="TNR11" x="0" y="0" width="465" height="14"/>
				<textElement textAlignment="Justified">
					<font fontName="Times New Roman" size="10" isBold="false"/>
				</textElement>
				<textFieldExpression class="java.lang.String"><![CDATA["The sum payable by CNR will be available for collection, during the Contractor’s research stay in Italy, at any branch of the Banca Nazionale del Lavoro (BNL) on the Italian territory."]]></textFieldExpression>
			</textField>
		</band>
		<band height="18" splitType="Prevent">
			<printWhenExpression><![CDATA[$F{IM_RIMB_SPESE_ANT}!= null && $F{FL_PAGAMENTO_FINE_VISITA}.equals( "N" )]]></printWhenExpression>
			<textField isStretchWithOverflow="true" isBlankWhenNull="true">
				<reportElement key="textField-2" style="TNR11" x="0" y="0" width="465" height="14"/>
				<textElement textAlignment="Justified">
					<font fontName="Times New Roman" size="10" isBold="false"/>
				</textElement>
				<textFieldExpression class="java.lang.String"><![CDATA["The CNR will pay the due sum in two installments:"]]></textFieldExpression>
			</textField>
		</band>
		<band height="18" splitType="Prevent">
			<printWhenExpression><![CDATA[$F{IM_RIMB_SPESE_ANT}!= null && $F{FL_PAGAMENTO_FINE_VISITA}.equals( "N" )]]></printWhenExpression>
			<textField isStretchWithOverflow="true" isBlankWhenNull="true">
				<reportElement key="textField-2" style="TNR11" x="13" y="0" width="453" height="14"/>
				<textElement textAlignment="Justified">
					<font fontName="Times New Roman" size="10"/>
				</textElement>
				<textFieldExpression class="java.lang.String"><![CDATA["The first of Euro "+new java.text.DecimalFormat("#,##0.00").format($F{IM_RIMB_SPESE_ANT})+" will be paid to the Contractor during "+($F{SESSO_AUTORIZZATO}.equals("M")?"his":"her")+" stay by any branch of the Banca Nazionale del Lavoro (BNL) on the Italian territory."]]></textFieldExpression>
			</textField>
			<textField isStretchWithOverflow="true" isBlankWhenNull="true">
				<reportElement key="textField-2" style="TNR11" x="0" y="0" width="13" height="12"/>
				<textElement textAlignment="Center">
					<font fontName="Times New Roman" size="10" isBold="false"/>
				</textElement>
				<textFieldExpression class="java.lang.String"><![CDATA["-"]]></textFieldExpression>
			</textField>
		</band>
		<band height="18" splitType="Prevent">
			<printWhenExpression><![CDATA[$F{IM_RIMB_SPESE_ANT}!= null &&$F{FL_PAGAMENTO_FINE_VISITA}.equals( "N" )]]></printWhenExpression>
			<textField isStretchWithOverflow="true" isBlankWhenNull="true">
				<reportElement key="textField-2" style="TNR11" x="13" y="0" width="453" height="14"/>
				<textElement textAlignment="Justified">
					<font fontName="Times New Roman" size="10"/>
				</textElement>
				<textFieldExpression class="java.lang.String"><![CDATA["The second of Euro "+new java.text.DecimalFormat("#,##0.00").format($F{IM_RIMB_SPESE})+" will be paid at the end of the stay by bank transfer directly onto the Contractor’s own bank account SWIFT Code "+$F{CODICE_SWIFT}+", IBAN Code "+$F{CODICE_IBAN}+"."]]></textFieldExpression>
			</textField>
			<textField isStretchWithOverflow="true" isBlankWhenNull="true">
				<reportElement key="textField-2" style="TNR11" x="0" y="0" width="13" height="12"/>
				<textElement textAlignment="Center">
					<font fontName="Times New Roman" size="10" isBold="false"/>
				</textElement>
				<textFieldExpression class="java.lang.String"><![CDATA["-"]]></textFieldExpression>
			</textField>
		</band>
		<band height="18" splitType="Prevent">
			<textField isStretchWithOverflow="true" isBlankWhenNull="true">
				<reportElement key="textField-2" style="TNR11" x="0" y="4" width="465" height="14"/>
				<textElement>
					<font fontName="Times New Roman" size="10" isBold="false"/>
				</textElement>
				<textFieldExpression class="java.lang.String"><![CDATA["ART. 6 – Renunciation"]]></textFieldExpression>
			</textField>
		</band>
		<band height="18" splitType="Prevent">
			<textField isStretchWithOverflow="true" isBlankWhenNull="true">
				<reportElement key="textField-2" style="TNR11" x="0" y="0" width="465" height="14"/>
				<textElement textAlignment="Justified">
					<font fontName="Times New Roman" size="10" isBold="false"/>
				</textElement>
				<textFieldExpression class="java.lang.String"><![CDATA["The Contractor must communicate at once to the International Relations and Agreements Office of the CNR "+($F{SESSO_AUTORIZZATO}.equals("M")?"himself":"herself")+" renunciation to the research stay. Should the Contractor announce "+($F{SESSO_AUTORIZZATO}.equals("M")?"himself":"herself")+" renunciation, no money will payable by CNR. Should the Contractor reduce the duration of "+($F{SESSO_AUTORIZZATO}.equals("M")?"him":"her")+" approved stay, the sum of Euro "+new java.text.DecimalFormat("#,##0.00").format($F{IMPORTO_DIARIA_STR})+" per each day which have been cancelled, will have to be returned to CNR by bank transfer made payable to the following beneficiary:"]]></textFieldExpression>
			</textField>
		</band>
		<band height="18" splitType="Prevent">
			<textField isStretchWithOverflow="true" isBlankWhenNull="true">
				<reportElement key="textField-2" style="TNR11" x="0" y="0" width="465" height="14"/>
				<textElement textAlignment="Justified">
					<font fontName="Times New Roman" size="10" isBold="false"/>
				</textElement>
				<textFieldExpression class="java.lang.String"><![CDATA["Consiglio Nazionale delle Ricerche Banca Nazionale del Lavoro – Sportello CNR n. 6392 – Piazzale Aldo Moro, 7 00185 Roma - Italy - Account Number (conto di evidenza n.) 218155 under the name of 'Incassi e pagamenti giornalieri', with the indication 'Ufficio Accordi e Relazioni Internazionali - mandato n. (Mandate Number) …………………… – giorni non effettuati n. (Number of days of reduction) ……………….'"]]></textFieldExpression>
			</textField>
		</band>
		<band height="18" splitType="Prevent">
			<textField isStretchWithOverflow="true" isBlankWhenNull="true">
				<reportElement key="textField-2" style="TNR11" x="0" y="4" width="465" height="14"/>
				<textElement>
					<font fontName="Times New Roman" size="10" isBold="false"/>
				</textElement>
				<textFieldExpression class="java.lang.String"><![CDATA["ART. 7 – Intellectual Property and Rights"]]></textFieldExpression>
			</textField>
		</band>
		<band height="18" splitType="Prevent">
			<textField isStretchWithOverflow="true" isBlankWhenNull="true">
				<reportElement key="textField-2" style="TNR11" x="0" y="0" width="465" height="14"/>
				<textElement textAlignment="Justified">
					<font fontName="Times New Roman" size="10" isBold="false"/>
				</textElement>
				<textFieldExpression class="java.lang.String"><![CDATA["See Art. "+$F{NUM_ARTICOLO_PROP_INTEL}+" of the Agreement of Scientific Cooperation signed between CNR and "+$F{ACRONIMO_ENTE_STR}+"."]]></textFieldExpression>
			</textField>
		</band>
		<band height="18" splitType="Prevent">
			<textField isStretchWithOverflow="true" isBlankWhenNull="true">
				<reportElement key="textField-2" style="TNR11" x="0" y="4" width="465" height="14"/>
				<textElement>
					<font fontName="Times New Roman" size="10" isBold="false"/>
				</textElement>
				<textFieldExpression class="java.lang.String"><![CDATA["ART. 8 – Personal Data Protection"]]></textFieldExpression>
			</textField>
		</band>
		<band height="18" splitType="Prevent">
			<textField isStretchWithOverflow="true" isBlankWhenNull="true">
				<reportElement key="textField-2" style="TNR11" x="0" y="0" width="465" height="14"/>
				<textElement textAlignment="Justified">
					<font fontName="Times New Roman" size="10" isBold="false"/>
				</textElement>
				<textFieldExpression class="java.lang.String"><![CDATA["For the safeguard of the Contractor, the provisions detailed by the Legislative Decree no. 196 of 30/06/2003 will be implemented with reference to Personal Data Protection."]]></textFieldExpression>
			</textField>
		</band>
		<band height="18" splitType="Prevent">
			<textField isStretchWithOverflow="true" isBlankWhenNull="true">
				<reportElement key="textField-2" style="TNR11" x="0" y="4" width="465" height="14"/>
				<textElement>
					<font fontName="Times New Roman" size="10" isBold="false"/>
				</textElement>
				<textFieldExpression class="java.lang.String"><![CDATA["ART. 9 – Place of Lawful Jurisdiction"]]></textFieldExpression>
			</textField>
		</band>
		<band height="18" splitType="Prevent">
			<textField isStretchWithOverflow="true" isBlankWhenNull="true">
				<reportElement key="textField-2" style="TNR11" x="0" y="0" width="465" height="14"/>
				<textElement textAlignment="Justified">
					<font fontName="Times New Roman" size="10" isBold="false"/>
				</textElement>
				<textFieldExpression class="java.lang.String"><![CDATA["Should any legal controversy arise, the Contractor declares "+($F{SESSO_AUTORIZZATO}.equals("M")?"his":"her")+" acceptance of the Tribunal of Rome as the place of lawful jurisdiction."]]></textFieldExpression>
			</textField>
		</band>
