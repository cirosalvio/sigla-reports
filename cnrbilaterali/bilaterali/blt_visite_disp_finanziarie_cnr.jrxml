	<queryString>
		<![CDATA[select
a.dt_ini_visita, a.dt_fin_visita, e.telef_respons_ita, e.fax_respons_ita, e.email_respons_ita,
b.denominazione_sede ds_terzo_autorizzato, e.email_trasmissione_passaporto, e.fl_atti_amministrativi,
c.ds_cdr, e.anno_ini, e.anno_fin, e.fl_viaggi_interni, h.ente_di_appartenenza, h.indirizzo_ente_di_appartenenza, h.cap_ente_di_appartenenza, h.fl_associato, e.im_diaria_contrib_sogg_ita, e.im_mensile_contrib_sogg_ita,
(select b.denominazione_sede from terzo b where b.cd_terzo = g.cd_respons_ita) responsabile_ita,
(select b.denominazione_sede from terzo b where b.cd_terzo = g.cd_respons_str) responsabile_str,
s.ti_sesso sesso_autorizzato,
(select h.via_sede from terzo h where c.cd_unita_organizzativa = h.cd_unita_organizzativa) indirizzo_istituto,
(select h.cap_comune_sede from terzo h where c.cd_unita_organizzativa = h.cd_unita_organizzativa) cap_istituto,
(select l.ds_comune from comune l where  h.pg_comune_ente_di_appartenenza =l.pg_comune) comune_ente_di_appartenenza,
(select l.cd_provincia from anagrafico s, comune l where s.cd_anag = h.cd_terzo and s.pg_comune_fiscale = l.pg_comune) provincia_istituto,
(select l.ds_comune from terzo h, comune l where c.cd_unita_organizzativa = h.cd_unita_organizzativa and h.pg_comune_sede = l.pg_comune) comune_istituto,
 e.email_trasmissione_passaporto,
e.acronimo_ente_str, f.ds_nazione ds_nazione_ente_str, e.nome_ente_str,
g.ds_progetto_ita, e.importo_diaria_ita, e.fl_spese_visto, e.cd_divisa_ita,
e.fl_rimborso_treno, e.importo_mensile_ita, h.fl_assimilato_dip
from blt_visite a, blt_autorizzati h, terzo b, cdr c, blt_accordi e, nazione f, blt_progetti g, anagrafico s
where a.cd_accordo = $P{cd_accordo}
and a.cd_progetto = $P{cd_progetto}
and a.cd_terzo = $P{cd_terzo}
and a.pg_autorizzazione = $P{pg_autorizzazione}
and a.pg_visita = $P{pg_visita}
and a.cd_accordo = h.cd_accordo
and a.cd_progetto = h.cd_progetto
and a.cd_terzo = h.cd_terzo
and h.cd_terzo = b.cd_terzo
and h.cd_cdr_terzo = c.cd_centro_responsabilita (+)
and h.cd_accordo = e.cd_accordo
and e.pg_nazione_str = f.pg_nazione
and a.cd_accordo = g.cd_accordo
and a.cd_progetto = g.cd_progetto
and s.cd_anag = b.cd_anag]]>
	</queryString>
