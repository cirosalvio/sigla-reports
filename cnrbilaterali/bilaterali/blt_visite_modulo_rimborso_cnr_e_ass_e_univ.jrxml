				<reportElement key="textField-2" style="TNR11" x="40" y="0" width="426" height="12"/>
				<textElement textAlignment="Justified">
					<font fontName="Times New Roman" size="10" isBold="false" isItalic="false"/>
				</textElement>
				<textFieldExpression class="java.lang.String"><![CDATA["i servizi resi dall'agenzia (spedizione biglietteria, spese di agenzia per concessione visto);"]]></textFieldExpression>
			</textField>
			<staticText>
				<reportElement key="staticText-1" style="TNRG11" x="25" y="0" width="15" height="12"/>
