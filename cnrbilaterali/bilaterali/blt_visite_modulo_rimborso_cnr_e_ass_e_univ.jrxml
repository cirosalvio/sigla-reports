				<textElement textAlignment="Justified">
					<font fontName="Times New Roman" size="10" isBold="true" isUnderline="true"/>
				</textElement>
				<textFieldExpression class="java.lang.String"><![CDATA["Non saranno rimborsate le seguenti spese:"]]></textFieldExpression>
			</textField>
		</band>
		<band height="15" splitType="Prevent">
			<textField isStretchWithOverflow="true" isBlankWhenNull="true">
				<reportElement key="textField-2" style="TNR11" x="40" y="0" width="426" height="12"/>
				<textElement textAlignment="Justified">
					<font fontName="Times New Roman" size="10" isBold="false" isItalic="false"/>
				</textElement>
				<textFieldExpression class="java.lang.String"><![CDATA["il viaggio con mezzo proprio o a noleggio;"]]></textFieldExpression>
			</textField>
			<staticText>
				<reportElement key="staticText-1" style="TNRG11" x="25" y="0" width="15" height="12"/>
				<textElement textAlignment="Center">
					<font fontName="Times New Roman" size="10" isBold="false"/>
				</textElement>
				<text><![CDATA[-]]></text>
			</staticText>
		</band>
		<band height="15" splitType="Prevent">
