	<queryString>
		<![CDATA[select
a.dt_ini_visita, a.dt_fin_visita, s.codice_fiscale, s.dt_nascita, l.ds_comune comune_nascita, l.cd_provincia provincia_autorizzato, e.anno_ini, e.anno_fin, e.fl_viaggi_interni, e.im_diaria_contrib_sogg_ita, e.im_mensile_contrib_sogg_ita, b.denominazione_sede ds_terzo_autorizzato, a.num_prot_attrib_incarico, a.dt_prot_attrib_incarico, e.acronimo_ente_str, f.ds_nazione ds_nazione_ente_str, e.nome_ente_str, a.num_prot_accett_ente_str,
(select b.denominazione_sede from terzo b where b.cd_terzo = g.cd_respons_ita) responsabile_ita,
(select b.denominazione_sede from terzo b where b.cd_terzo = g.cd_respons_str) responsabile_str,
s.ti_sesso sesso_autorizzato, a.dt_prot_accett_ente_str, a.luogo_visita, e.num_articolo_prop_intel,
g.ds_progetto_ita, e.importo_diaria_ita, e.fl_spese_visto, e.cd_divisa_ita, e.data_firma_accordo,
e.fl_rimborso_treno, e.importo_mensile_ita, h.fl_assimilato_dip, e.data_firma_addendum
from blt_visite a, blt_autorizzati h, terzo b, blt_accordi e, nazione f, blt_progetti g, anagrafico s, comune l
where a.cd_accordo = $P{cd_accordo}
and a.cd_progetto = $P{cd_progetto}
and a.cd_terzo = $P{cd_terzo}
and a.pg_autorizzazione = $P{pg_autorizzazione}
and a.pg_visita = $P{pg_visita}
and a.cd_accordo = h.cd_accordo
and a.cd_progetto = h.cd_progetto
and a.cd_terzo = h.cd_terzo
and h.cd_terzo = b.cd_terzo
and h.cd_accordo = e.cd_accordo
and e.pg_nazione_str = f.pg_nazione
and a.cd_accordo = g.cd_accordo
and a.cd_progetto = g.cd_progetto
and s.cd_anag = b.cd_anag
and s.pg_comune_nascita = l.pg_comune]]>
	</queryString>
