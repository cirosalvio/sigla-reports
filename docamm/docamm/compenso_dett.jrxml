	</parameter>
	<queryString>
		<![CDATA[select esercizio_ori_obbligazione, pg_obbligazione, im_totale_riga_compenso
from compenso_riga a
WHERE a.ESERCIZIO = $P{Esercizio}
  AND a.CD_CDS like $P{CDS}
  AND a.CD_UNITA_ORGANIZZATIVA = $P{UO}
  AND a.PG_COMPENSO = $P{Pg_compenso}
order by pg_obbligazione]]>
	</queryString>
